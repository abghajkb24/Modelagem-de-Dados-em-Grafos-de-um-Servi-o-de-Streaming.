// Cypher script to populate the database\n\nCREATE (u1:User {name: 'Alice', age: 30})\nCREATE (u2:User {name: 'Bob', age: 25})\nCREATE (u3:User {name: 'Charlie', age: 35})\n\nCREATE (m1:Movie {title: 'Inception', year: 2010})\nCREATE (m2:Movie {title: 'The Matrix', year: 1999})\nCREATE (m3:Movie {title: 'Interstellar', year: 2014})\n\nCREATE (u1)-[:FRIENDS]->(u2)\nCREATE (u2)-[:FRIENDS]->(u3)\nCREATE (u1)-[:WATCHED]->(m1)\nCREATE (u2)-[:WATCHED]->(m2)\nCREATE (u3)-[:WATCHED]->(m3);
